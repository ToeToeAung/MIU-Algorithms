package W1D1Labs;

public class Prob3_Test {

	public static void main(String[] args) {
		
		Prob3 prob3 = new Prob3();

        // Test case 1: Anagram strings
        String s1 = "anagram";
        String t1 = "nagaram";
        System.out.println("Test Case 1: " + s1 + " and " + t1 + " -> " + prob3.isAnagram(s1, t1)); // Output: true

        // Test case 2: Non-anagram strings
        String s2 = "rat";
        String t2 = "car";
        System.out.println("Test Case 2: " + s2 + " and " + t2 + " -> " + prob3.isAnagram(s2, t2)); // Output: false

        // Test case 3: Single character strings (Anagram)
        String s3 = "a";
        String t3 = "a";
        System.out.println("Test Case 3: " + s3 + " and " + t3 + " -> " + prob3.isAnagram(s3, t3)); // Output: true

        // Test case 4: Strings with different lengths
        String s4 = "hello";
        String t4 = "hell";
        System.out.println("Test Case 4: " + s4 + " and " + t4 + " -> " + prob3.isAnagram(s4, t4)); // Output: false

        // Test case 5: Strings with the same characters in different orders
        String s5 = "listen";
        String t5 = "silent";
        System.out.println("Test Case 5: " + s5 + " and " + t5 + " -> " + prob3.isAnagram(s5, t5)); // Output: true

        // Test case 6: Longer strings
        String s6 = "aabbcc";
        String t6 = "abcabc";
        System.out.println("Test Case 6: " + s6 + " and " + t6 + " -> " + prob3.isAnagram(s6, t6)); // Output: true

        // Test case 7: More complex non-anagram
        String s7 = "hello";
        String t7 = "helloo";
        System.out.println("Test Case 7: " + s7 + " and " + t7 + " -> " + prob3.isAnagram(s7, t7)); // Output: false

	}

}
