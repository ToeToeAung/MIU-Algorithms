package W1D1Labs;

import java.util.List;

public class Prob2_Test {

	public static void main(String[] args) {
		
	}
	
	private static void testReverseList(List<Integer> input, List<Integer> expected) {
		 Prob2 prob2=new Prob2();
	    List<Integer> result = prob2.reversedList(input);
	   
	    
	    if (result.equals(expected)) {
	        System.out.println("Test Passed: " + input + " reversed to " + result);
	    } else {
	        System.out.println("Test Failed: Expected " + expected + ", but got " + result);
	    }
	}

}
