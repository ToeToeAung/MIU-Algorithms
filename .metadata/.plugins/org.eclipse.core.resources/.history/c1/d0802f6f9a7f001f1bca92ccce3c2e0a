package W1D1Labs;

public class Prob1_Test {

	public static void main(String[] args) {
		  Prob1 stack = new Prob1();

	        // Test case 1: Check if stack is empty
	        System.out.println("Test Case 1: Is the stack empty? " + stack.empty()); // Expected: true

	        // Test case 2: Push elements onto the stack
	        stack.push(1);
	        stack.push(2);
	        stack.push(3);
	        System.out.println("Test Case 2: Top element after pushing 1, 2, 3: " + stack.top()); // Expected: 3

	        // Test case 3: Pop an element
	        System.out.println("Test Case 3: Popped element: " + stack.pop()); // Expected: 3
	        System.out.println("Test Case 3: Top element after pop: " + stack.top()); // Expected: 2

	}
}
